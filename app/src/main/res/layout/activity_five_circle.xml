<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.ftkj.customview_test.FiveCircleActivity">
<com.example.ftkj.customview_test.customview.fiveRings.FiveRingsLayout
    android:layout_gravity="center"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.example.ftkj.customview_test.customview.fiveRings.ColorCircleView
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:color="#3639ff"
        />
    <com.example.ftkj.customview_test.customview.fiveRings.ColorCircleView
        android:layout_width="90dp"
        android:layout_height="90dp"
        />
    <com.example.ftkj.customview_test.customview.fiveRings.ColorCircleView
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:color="#f00"
        />
    <com.example.ftkj.customview_test.customview.fiveRings.ColorCircleView
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:color="#dfc42a"
        />
    <com.example.ftkj.customview_test.customview.fiveRings.ColorCircleView
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:color="#5cff6c"
        />
</com.example.ftkj.customview_test.customview.fiveRings.FiveRingsLayout>
</FrameLayout>
